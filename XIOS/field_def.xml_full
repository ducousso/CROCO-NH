         <!-- SALINITY --> 
         <field id="AKs" long_name="salinity vertical diffusion coefficient" unit="meter2 second-1" grid_ref="w_3D"/>
         <!-- END SALINITY --> 
#endif
#ifdef GLS_MIXING
         <!-- GLS_MIXING -->
         <field id="tke" long_name="turbulent kinetic energy" unit="meter2 second-2" grid_ref="w_3D"/>
