         <!-- END SEDIMENT -->
#endif
#ifdef WET_DRY
         <field id="vmask_wet" long_name="" unit=""  />
#endif
     </field_group>
    
     <!-- W grid -->
     <field_group id="w" grid_ref="w_2D">
         <field id="omega" long_name="S-coordinate vertical momentum component" unit="meter second-1" grid_ref="w_3D"/>
