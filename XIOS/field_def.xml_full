#endif
#ifdef WET_DRY
         <field id="umask_wet" long_name="" unit=""  />
#endif
     </field_group>

     <!-- V grid -->
     <field_group id="v" grid_ref="v_2D">
         <field id="vbar" long_name="vertically integrated v-momentum component" unit="meter second-1"/>
